{
  "lookup": {
    "packages": ["@latitude/ui"],
    "components": ["Text"]
  },
  "migr8rules": [
    {
      "package": "@latitude/ui",
      "importType": "named",
      "component": "Text",
      "importTo": {
        "component": "Text",
        "importStm": "@latitude/text",
        "importType": "named"
      },
      "rules": [
        {
          "order": 1,
          "match": [
            {
              "type": "heading",
              "size": "large"
            }
          ],
          "remove": ["type", "size"],
          "set": {
            "variant": "headingLarge"
          }
        },
        {
          "order": 2,
          "match": [
            {
              "type": "body",
              "size": "medium"
            }
          ],
          "remove": ["type", "size"],
          "set": {
            "variant": "bodyMedium"
          }
        },
        {
          "order": 999,
          "match": [{}],
          "set": {},
          "remove": []
        }
      ]
    }
  ]
}