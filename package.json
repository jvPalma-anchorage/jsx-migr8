{
  "name": "jsx-migr8",
  "version": "1.0.0",
  "description": "Interactive CLI that scans a code-base, analyses JSX component usage and applies codemod-level migrations with preview/diff tooling.",
  "author": "Jo√£o Vieira Palma <joaovieirapalma@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jvPalma-anchorage/jsx-migr8.git"
  },
  "homepage": "https://github.com/jvPalma-anchorage/jsx-migr8#readme",
  "bugs": {
    "url": "https://github.com/jvPalma-anchorage/jsx-migr8/issues"
  },
  "keywords": [
    "codemod",
    "jsx",
    "react",
    "migration",
    "cli",
    "ast",
    "recast",
    "typescript"
  ],
  "packageManager": "yarn@4.2.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/cli/index.ts",
    "clean": "rm -f ./queue/props-usage.json && rm -f ./queue/usage.json",
    "deep-clean": "yarn clean && rm -f ./queue/component-spec.json",
    "dev": "yarn start",
    "dry-run": "yarn start --dry-run",
    "yolo": "yarn start --yolo",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@babel/core": "7.27.4",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@inquirer/prompts": "7.5.3",
    "dotenv": "16.5.0",
    "fast-glob": "3.3.3",
    "jscodeshift": "17.3.0",
    "recast": "0.23.11",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/node": "22.15.29",
    "@types/yargs": "17.0.33",
    "chalk": "5.4.1",
    "diff": "8.0.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "module-alias": "2.2.3",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.5.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
