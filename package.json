{
  "name": "jsx-migr8",
  "private": true,
  "version": "1.0.0",
  "description": "Interactive CLI that scans a code-base, analyses JSX component usage and applies codemod-level migrations with preview/diff tooling.",
  "author": "João Vieira Palma <joaovieirapalma@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jvPalma-anchorage/jsx-migr8.git"
  },
  "homepage": "https://github.com/jvPalma-anchorage/jsx-migr8#readme",
  "bugs": {
    "url": "https://github.com/jvPalma-anchorage/jsx-migr8/issues"
  },
  "keywords": [
    "codemod",
    "jsx",
    "react",
    "migration",
    "cli",
    "ast",
    "recast",
    "typescript"
  ],
  "packageManager": "yarn@4.2.0",
  "type": "module",
  "main": "dist/index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "tsx src/cli/index.ts",
    "clean": "rm -f ./queue/props-usage.json && rm -f ./queue/usage.json",
    "deep-clean": "yarn clean && rm -f ./queue/component-spec.json",
    "dev": "yarn start",
    "dry-run": "yarn start --dry-run",
    "yolo": "yarn start --yolo",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backup": "jest src/backup/__tests__",
    "test:backup:watch": "jest src/backup/__tests__ --watch",
    "test:backup:coverage": "jest src/backup/__tests__ --coverage",
    "test:integration": "tsx src/cli/index.ts --help && echo '✅ CLI integration test passed'",
    "test:integration:full": "tsx src/__tests__/integration/run-integration-tests.ts",
    "test:integration:cli": "jest --testPathPattern=cli-workflows.test.ts",
    "test:integration:snapshots": "jest --testPathPattern=transformation-snapshots.test.ts",
    "test:integration:performance": "jest --testPathPattern=performance-regression.test.ts --maxWorkers=1",
    "test:integration:errors": "jest --testPathPattern=error-scenarios.test.ts",
    "test:integration:backup": "jest --testPathPattern=backup-rollback.test.ts",
    "test:integration:node": "jest --testPathPattern=node-compatibility.test.ts",
    "test:integration:update-snapshots": "jest --testPathPattern=snapshot --updateSnapshot",
    "test:imports": "tsx -e \"import('./src/backup/index.ts').then(() => console.log('✅ All backup imports work')).catch(err => { console.error('❌ Import test failed:', err); process.exit(1); })\"",
    "test:migration-scenarios": "jest src/__tests__/migration-scenarios/migration-scenarios.test.ts",
    "test:migration-scenarios:watch": "jest src/__tests__/migration-scenarios/migration-scenarios.test.ts --watch",
    "validate:migration-scenarios": "tsx src/__tests__/migration-scenarios/validate-scenarios.ts",
    "test:cli": "node test-cli-interaction.js",
    "test:cli:advanced": "node test-cli-advanced.js",
    "test:cli:all": "node test-cli-advanced.js --all",
    "test:cli:interactive": "./run-cli-tests.sh",
    "demo:interactive-diff": "tsx src/cli/interactive-diff/cli-demo.ts",
    "demo:interactive-diff:example": "tsx src/cli/interactive-diff/example.ts",
    "api:dev": "yarn workspace @jsx-migr8/api dev",
    "api:build": "yarn workspace @jsx-migr8/api build",
    "api:start": "yarn workspace @jsx-migr8/api start",
    "api:example": "yarn workspace @jsx-migr8/api tsx examples/client.example.ts",
    "web:dev": "yarn workspace @jsx-migr8/web dev",
    "web:build": "yarn workspace @jsx-migr8/web build",
    "web:preview": "yarn workspace @jsx-migr8/web preview",
    "dev:all": "concurrently \"yarn api:dev\" \"yarn web:dev\"",
    "dev:termux": "./scripts/termux-dev.sh",
    "start:termux": "./termux-quickstart.sh",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build:all": "yarn build:cli && yarn build:api && yarn build:web",
    "build:cli": "tsup src/cli/index.ts --format esm --dts",
    "build:api": "yarn workspace @jsx-migr8/api build",
    "build:web": "yarn workspace @jsx-migr8/web build",
    "clean:all": "lerna clean -y && yarn clean",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "install:all": "yarn install && lerna bootstrap",
    "lint:all": "lerna run lint",
    "publish:all": "lerna publish",
    "test:all": "lerna run test",
    "version:all": "lerna version"
  },
  "dependencies": {
    "@babel/core": "7.27.4",
    "@babel/parser": "7.27.4",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@inquirer/prompts": "7.5.3",
    "blessed": "^0.1.81",
    "cli-boxes": "^3.0.0",
    "dotenv": "16.5.0",
    "fast-glob": "3.3.3",
    "jscodeshift": "17.3.0",
    "lru-cache": "^11.1.0",
    "node-fetch": "^2.7.0",
    "recast": "0.23.11",
    "terminal-size": "^4.0.0",
    "ws": "^8.18.2",
    "yargs": "17.7.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@jest/globals": "30.0.0",
    "@types/blessed": "^0.1.25",
    "@types/jest": "30.0.0",
    "@types/node": "22.15.29",
    "@types/yargs": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "chalk": "^5.4.1",
    "concurrently": "^8.2.2",
    "diff": "8.0.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "jest": "30.0.0",
    "lerna": "^8.1.2",
    "module-alias": "2.2.3",
    "prettier": "3.3.3",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.5.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
