  "rules": [
    {
      "component": "ExpansionPanel",
      "match": [],
      "rename": {
        "ExpansionPanel": "Accordion"
      },
      "importFrom": "@material-ui/core",
      "importTo": "@mui/material"
    },
    {
      "component": "ExpansionPanelSummary",
      "match": [],
      "rename": {
        "ExpansionPanelSummary": "AccordionSummary"
      },
      "importFrom": "@material-ui/core",
      "importTo": "@mui/material"
    },
    {
      "component": "ExpansionPanelDetails",
      "match": [],
      "rename": {
        "ExpansionPanelDetails": "AccordionDetails"
      },
      "importFrom": "@material-ui/core",
      "importTo": "@mui/material"
    },
    {
      "component": "Hidden",
      "match": [],
      "remove": ["smDown", "mdUp", "lgUp", "xlUp", "smUp", "mdDown", "lgDown", "xlDown"],
      "set": {
        "sx": "{ display: { xs: 'none', sm: 'block' } }"
      },
      "replaceWith": {
        "code": "<Box sx={{ display: { xs: 'none', sm: 'block' } }}>{CHILDREN}</Box>",
        "INNER_PROPS": [],
        "OUTER_PROPS": []
      },
      "importFrom": "@material-ui/core",
      "importTo": "@mui/material"
    },
    {
      "component": "Skeleton",
      "match": [{ "variant": "rect" }],
      "rename": {
        "variant": "variant"
      },
      "set": {
        "variant": "rectangular"
      },
      "importFrom": "@material-ui/lab",
      "importTo": "@mui/material"
    },
    {
      "component": "Skeleton",
      "match": [{ "variant": "circle" }],
      "rename": {
        "variant": "variant"
      },
      "set": {
        "variant": "circular"
      },
      "importFrom": "@material-ui/lab",
      "importTo": "@mui/material"
    },
    {
      "component": "DatePicker",
      "match": [],
      "remove": ["renderInput"],
      "rename": {
        "onChange": "onChange",
        "value": "value"
      },
      "replaceWith": {
        "code": "<DatePicker {...OUTER_PROPS} slots={{ textField: TextField }} />",
        "INNER_PROPS": [],
        "OUTER_PROPS": ["label", "value", "onChange"]
      },
      "importFrom": "@material-ui/pickers",
      "importTo": "@mui/x-date-pickers"
    },
    {
      "component": "TimePicker",
      "match": [],
