    {
      "function": "makeStyles",
      "match": [],
      "replaceWith": {
        "code": "// Consider migrating to emotion/styled or sx prop instead of makeStyles\n// makeStyles is deprecated in MUI v5\nconst useStyles = makeStyles((theme) => ({...OUTER_PROPS}))",
        "note": "makeStyles is deprecated in MUI v5. Consider using emotion/styled, sx prop, or styled() API."
      },
      "importFrom": "@material-ui/core/styles",
      "importTo": "@mui/styles"
    },
    {
      "function": "withStyles",
      "match": [],
      "replaceWith": {
        "code": "// Consider migrating to styled() API instead of withStyles\n// withStyles is deprecated in MUI v5\nconst StyledComponent = styled(Component)(({ theme }) => ({...OUTER_PROPS}))",
