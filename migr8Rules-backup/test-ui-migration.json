          "match": [{ "size": "large", "weight": "bold" }],
          "remove": ["size", "weight"],
          "set": { "variant": "h1" }
        },
        {
          "order": 2,
          "match": [{ "size": "medium" }],
          "remove": ["size"],
          "set": { "variant": "body1" }
